ex:SuperShape
	a sh:Shape ;
	sh:property [
		sh:predicate ex:property ;
		sh:minCount 1 ;
	] .

ex:ExampleAndShape
	a sh:Shape ;
	sh:property [
		sh:predicate ex:property ;
		sh:maxCount 1;
	] .


ex:ValidInstance1
	sh:nodeShape ex:ExampleAndShape;
	ex:property "One" .